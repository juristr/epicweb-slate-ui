{
  "name": "@slateui/icons",
  "version": "1.0.0",
  "description": "Icon system for SlateUI",
  "scripts": {
    "build": "tsup ./index.ts --format esm,cjs --dts",
    "dev": "tsup ./index.ts --format esm,cjs --watch --dts",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "icons",
    "slateui"
  ],
  "author": "",
  "license": "ISC"
}
